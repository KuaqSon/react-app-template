{
  "name": "react-app-template",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "npm": ">=8.0.0",
    "node": ">=16.0.0"
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@mantine/core": "^5.7.1",
    "@mantine/dates": "^5.7.1",
    "@mantine/form": "^5.7.1",
    "@mantine/hooks": "^5.7.1",
    "@mantine/modals": "^5.7.1",
    "@mantine/notifications": "^5.7.1",
    "@tabler/icons": "^1.110.0",
    "@tanstack/react-query": "^4.14.5",
    "axios": "^1.1.3",
    "dayjs": "^1.11.6",
    "immer": "^9.0.16",
    "nanoid": "^4.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.6.0",
    "react-router-dom": "6.4.3",
    "react-scripts": "5.0.1",
    "web-vitals": "^3.0.4",
    "zustand": "^4.1.4"
  },
  "scripts": {
    "start": "export PORT=3088 && react-scripts start",
    "build": "GENERATE_SOURCEMAP=false react-scripts build",
    "build:dist": "GENERATE_SOURCEMAP=false BUILD_PATH='./dist' react-scripts build --max-old-space-size=3072",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "format": "npx prettier --config .prettierrc --write -c src",
    "lint": "eslint ./src --fix && tsc --noEmit",
    "lint:staged": "tsc --noEmit && eslint --cache --fix",
    "audit": "npm run format && npm run lint"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@trivago/prettier-plugin-sort-imports": "^3.4.0",
    "@types/jest": "^29.2.2",
    "@types/node": "^18.11.9",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "eslint": "^8.26.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-mantine": "^2.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.1.4",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-testing-library": "^5.9.1",
    "prettier": "^2.7.1",
    "sass": "^1.56.0",
    "typescript": "^4.8.4"
  }
}
