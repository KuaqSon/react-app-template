{
  "name": "react-app-template",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "npm": ">=8.0.0",
    "node": ">=16.0.0"
  },
  "dependencies": {
    "@emotion/react": "^11.10.6",
    "@mantine/core": "^5.10.5",
    "@mantine/dates": "^5.10.5",
    "@mantine/form": "^5.10.5",
    "@mantine/hooks": "^5.10.5",
    "@mantine/modals": "^5.10.5",
    "@mantine/notifications": "^5.10.5",
    "@tabler/icons": "^1.110.0",
    "@tanstack/react-query": "^4.24.10",
    "axios": "^1.3.4",
    "dayjs": "^1.11.7",
    "immer": "^9.0.19",
    "nanoid": "^4.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.7.1",
    "react-router-dom": "6.8.2",
    "react-scripts": "5.0.1",
    "web-vitals": "^3.1.1",
    "zustand": "^4.3.5"
  },
  "scripts": {
    "start": "export PORT=3088 && react-scripts start",
    "build": "GENERATE_SOURCEMAP=false react-scripts build",
    "build:dist": "GENERATE_SOURCEMAP=false BUILD_PATH='./dist' react-scripts build --max-old-space-size=3072",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "format": "npx prettier --config .prettierrc --write -c src",
    "lint": "eslint ./src --fix && tsc --noEmit",
    "lint:staged": "tsc --noEmit && eslint --cache --fix",
    "audit": "npm run format && npm run lint"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.14.2",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "eslint": "^8.35.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-testing-library": "^5.10.2",
    "prettier": "^2.8.4",
    "sass": "^1.58.3",
    "typescript": "^4.9.5"
  }
}
